package com.example.t3.comment.model;


import java.util.Date;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
public class Comment {
	  private int c_no;
	  private int d_pageno;
	  private String c_writer;
	  private String c_writer_email;
	  private String c_content;
	  private Date c_date;
	public Comment(int c_no, int d_pageno, String c_writer, String c_writer_email, String c_content) {
		super();
		this.c_no = c_no;
		this.d_pageno = d_pageno;
		this.c_writer = c_writer;
		this.c_writer_email = c_writer_email;
		this.c_content = c_content;
		this.c_date = new Date();
	}

	
}
